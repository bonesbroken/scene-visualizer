{
    "version": "1.0.1",
    "name": "Scene Collection Visualizer",
    "buildPath": "dist/",
    "permissions": ["slobs.scenes-sources", "slobs.scene-transitions", "slobs.external-links", "slobs.streaming", "slobs.scene-collections", "slobs.obs-settings", "slobs.notifications"],
    "supportedPlatforms": ["twitch", "youtube", "facebook"],
    "icon": "./assets/img/app-sq.jpg",
    "sources": [
      {
        "type": "browser_source",
        "name": "Scene Collection Visualizer",
        "id": "scene-collection-visualizer",
        "about": {
          "description": "Generates a video of your scene collection. Do not add this source to your scene manually.",
          "bannerImage": "./assets/img/app-icon.jpg",
          "bullets": [
            "Select your scenes & set video duration"
          ]
        },
        "file": "sceneCollection.html",
        "initialSize": {
          "type": "relative",
          "width": 1,
          "height": 1
        }
      }
    ],
    "pages": [
      {
        "slot": "top_nav",
        "file": "settings.html",
        "persistent": true,
        "allowPopout": false,
        "popOutSize": {
          "width": 800,
          "height": 800
        }
      }
    ]
  }